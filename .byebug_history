exit
users_path
users_path(@user)
user_path(@user)
root_path
root
user_path
@user
exit
            self.user.happiness += self.attraction.happiness_rating
            self.user.nausea += self.attraction.nausea_rating
self.user.tickets -= self.attraction.tickets
self.user.tickets >= self.attraction.tickets
exit
self.user.tickets < self.attraction.tickets
self.attraction.tickets
self.user.tickets
